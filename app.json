{
  "expo": {
    "name": "Pack&Trip",
    "slug": "proyectoMAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/trips.png",
    "scheme": "proyectomai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCwphdALcMvQ9biNO2WjhdPoj24_FCJwDo"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Esta app necesita acceso a tu ubicaci√≥n para mostrar mapas y direcciones."
      }
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-trips-background.png",
        "backgroundImage": "./assets/images/android-trips-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.bryanvaldy.proyectoMAI",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCwphdALcMvQ9biNO2WjhdPoj24_FCJwDo"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",

      [
        "expo-splash-screen",
        {
          "image": "./assets/images/trips.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-location"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "b3014ce6-b49a-4e79-9311-c250f7098b36"
      }
    },

    "owner": "bryanvaldy"
  }
}
